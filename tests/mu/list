assert_eq "(mu:car '(1))" "1"
assert_eq "(mu:car ())" ":nil"
assert_eq "(mu:cdr '(1 2))" "(2)"
assert_eq "(mu:cdr ())" ":nil"
assert_eq "(mu:cons 1 ())" "(1)"
assert_eq "(mu:cons 1 2)" "(1 . 2)"
assert_eq "(mu:length '(1))" "1"
assert_eq "(mu:length ())" "0"
assert_eq "(mu:nth 0 '(1 . 2))" "1"
assert_eq "(mu:nth 0 '(1 2))" "1"
assert_eq "(mu:nth 0 ())" ":nil"
assert_eq "(mu:nth 1 '(1 . 2))" "2"
assert_eq "(mu:nth 1 '(1 2))" "2"
assert_eq "(mu:nth 1 ())" ":nil"
assert_eq "(mu:nth 3 '(1 2))" ":nil"
assert_eq "(mu:nthcdr 0 '(1 . 2))" "(1 . 2)"
assert_eq "(mu:nthcdr 0 '(1 2))" "(1 2)"
assert_eq "(mu:nthcdr 0 ())" ":nil"
assert_eq "(mu:nthcdr 1 '(1 . 2))" "2"
assert_eq "(mu:nthcdr 1 '(1 2))" "(2)"
assert_eq "(mu:nthcdr 1 ())" ":nil"
assert_eq "(mu:nthcdr 3 '(1 2))" ":nil"