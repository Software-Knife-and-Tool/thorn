(mu:sv-len "abcde")	5
(mu:sv-len "abcdeghi")	8
(mu:sv-len "abcdegh")	7
(mu:sv-ref "abcde" 2)	#\c
(mu:sv-type "abcde")	:char
(mu:sv-len #(:t 1 a -3))	3
(mu:sv-ref #(:t 1 a -3) 1)	a
(mu:sv-type #(:byte 1 2 3))	:byte
(mu:sv-len #(:byte 1 2 3))	3
(mu:sv-ref #(:byte 1 2 3) 1)	2
(mu:sv-type #(:char #\1 #\2 #\3))	:char
(mu:sv-len #(:char #\1 #\2 #\3))	3
(mu:sv-ref #(:char #\1 #\2 #\3) 1)	#\2
(mu:sv-len #(:fixnum 1 2 -3))	3
(mu:sv-ref #(:fixnum 1 2 -3) 1)	2
(mu:sv-type #(:fixnum 1 2 -3))	:fixnum
(mu:sv-len #(:float 1.0 2.0 -3.0))	3
(mu:sv-ref #(:float 1.0 2.0 -3.0) 1)	2.0000
(mu:sv-type #(:float 1.0 2.0 -3.0))	:float
(mu:make-sv :byte '(1 2 3))	#(:byte 1 2 3)
(mu:make-sv :t '(1 2 3))	#(:t 1 2 3)
(mu:make-sv :fixnum '(1 2 3))	#(:fixnum 1 2 3)
(mu:make-sv :float '(1.0 2.0 -3.0))	#(:float 1.0000 2.0000 -3.0000)
