(mu:type-of core:apply)	:func
((:lambda () (core:compile '(define-core-function core:a-defun (a) a)) (core:a-defun 'a-defun)))	a-defun
((:lambda () (core:compile '(define-core-function core:a-defun (a b) (mu:cons a b))) (core:a-defun 'a-defun 'b-defun)))	(a-defun . b-defun)
((:lambda () (core:compile '(define-core-function core:a-defun (&rest a) a)) (core:apply core:a-defun '(1 2 3))))	(1 2 3)
((:lambda () (core:compile '(define-core-function core:a-defun (a &rest b) (mu:cons a b))) (core:apply core:a-defun '(0 1 2 3))))	(0 1 2 3)
((:lambda () (core:compile '(define-core-function core:a-defun (a b &rest c) (mu:cons b (mu:cons a c)))) (core:apply core:a-defun '(0 1 2 3))))	(1 0 2 3)
(core:apply (core:apply (core:compile '(define-core-lambda (a) (define-core-lambda (b) (mu:fx-add a b)))) '(5)) '(10))	15
