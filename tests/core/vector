(mu:type-of core:svref)	:func
(mu:type-of core:vector-type)	:func
(core:svref "abcde" 2)	#\c
(core:svref #(:byte 1 2 3) 1)	2
(core:svref #(:char #\1 #\2 #\3) 1)	#\2
(core:svref #(:fixnum 1 2 -3) 1)	2
(core:svref #(:float 1.0 2.0 -3.0) 1)	2.0000
(core:svref #(:t 1 a -3) 1)	a
(core:vector-type "abcde")	:char
(core:vector-type #(:byte 1 2 3))	:byte
(core:vector-type #(:char #\1 #\2 #\3))	:char
(core:vector-type #(:fixnum 1 2 -3))	:fixnum
(core:vector-type #(:float 1.0 2.0 -3.0))	:float
