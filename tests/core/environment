(mu:type-of core:add-env)	:func
(mu:type-of core:env-size)	:func
(mu:type-of core:envp)	:func
(mu:type-of core:get-env)	:func
(mu:type-of core:make-env)	:func
(core:type-of (core:make-env ()))	env
(core:envp (core:make-env ()))	:t
(core:add-env (core:make-env ()) 'a 'b)	1
(core:env-size (core:make-env '((a . b))))	1
(core:env-size (core:make-env ()))	0
(core:get-env (core:make-env '((a . b))) 'a)	b
(core:make-env '((a . b)))	#s(:<ctype> env ((:symbols . #<:map [size:2, tag:1]>)))
