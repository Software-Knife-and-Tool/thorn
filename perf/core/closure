(core:compile (:quote (define-core-lambda ())))
(core:compile (:quote (define-core-lambda (a) a)))
(core:compile (:quote (define-core-lambda (&rest c))))
(core:compile (:quote ((define-core-lambda (a b) (mu:fx-add a b)) 1 2)))
(core:compile (:quote ((define-core-lambda (a b &rest c) (mu:fx-add a b) c) 1 2)))
